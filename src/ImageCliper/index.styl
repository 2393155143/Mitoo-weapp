@import "../styles/index";
@import "../styles/mixins/animation";


.{$prefix}-avatar {
  position: absolute;
  width: 100%;
  height: 100%;
  top: 50%;
  transform: translateY(-50%);
  transform-origin: center;
}
.{$prefix}-avatar-mask {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: $background-color-mask;
  z-index: 10;
}
.{$prefix}-cut-mask {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  z-index: 10;
}
.{$prefix}-cut-area {
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
  transition: all $transition-time ease-in-out;
  width: 100%;
  overflow: hidden;
  &-avatar {
    position: absolute;
    width: 100%;
    height: 100%;
    z-index: 10;
    background-position: center;
    transform-origin: center;
    transition: top left $transition-time/10 linear;
  }
}
.{$prefix}-cut-tools {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 11;
  background-position: 0 0;
  background-image: url('data:image/png;base64,');
  background-size: 100%;
}
.{$prefix}-avatar-area {
  overflow: hidden;
  background-color: black;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  transform: translateY(100%);
  visibility: hidden;
  transition: transform $transition-time ease-in-out, visibility $transition-time ease-in-out;
  &-visible {
    visibility: visible;
    transform: translateY(0);
    transition: transform $transition-time ease-in-out, visibility $transition-time ease-in-out;
  }
  
}
.{$prefix}-operation-tools {
  position: absolute;
  bottom: 0;
  font-size: $font-size-base;
  display: flex;
  justify-content: space-between;
  align-items: center;
  z-index: $zindex-toast;
  width: 100%;
  &-left {
    padding: $padding-base;
    color: $color-text-secondary;
  }
  &-right {
    padding: $padding-base;
    color: white;
  }
}
.{$prefix}-canvas {
  visibility: hidden;
  opacity: 0;
  position: absolute;
  width: 0;
  height: 0;
  top: -100%;
}
