@import "../styles/default";
page {
  background-color: $background-color-base;
}
.{$prefix}-cell {
  font-size: $size-font-base;
  display: flex;
  box-pack: justify
  justify-content: space-between;
  &-title {
  }
  &-label {
    font-size: $size-font-small;
    color: $subsidiary-color;
  }
  &-right-container {
    display: flex;
    align-items: center;
    .{$prefix}-cell-link {
      color: $subsidiary-color;
    }
  }
}

