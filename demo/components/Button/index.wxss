.mit-button-wrap {
  padding: 18rpx;
}
.mit-button-wrap .mit-button {
  background-color: #ff6b22;
  color: #fff;
  font-size: 32rpx;
  padding: 6rpx 15rpx;
}
.mit-button-wrap .mit-button-small {
  padding: 2rpx 7rpx;
}
.mit-button-wrap .mit-button-large {
  padding: 6rpx 15rpx 7rpx 15rpx;
}
.mit-button-wrap .mit-button-circle {
  border-radius: 100rpx;
}
.mit-button-wrap .mit-button-hover {
  background-color: #cc561b;
}
