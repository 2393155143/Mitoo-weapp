.mit-checkbox-cell{display:flex;padding:30rpx;position:relative;border-bottom:1rpx solid #dddeee}@media (min-resolution:2dppx){.mit-checkbox-cell{border-bottom:none}.mit-checkbox-cell::after{content:'';position:absolute;background-color:#dddeee;display:block;z-index:2000;top:auto;right:auto;bottom:0;left:0;width:100%;height:1rpx;transform-origin:50% 100%;transform:scaleY(.5)}}.mit-checkbox-cell:active{background-color:#f3f3f3}.mit-checkbox-cell-last{border-bottom:0}.mit-checkbox-cell-last:after{display:none!important}@media (min-resolution:2dppx) and (min-resolution:3dppx){.mit-checkbox-cell::after{transform:scaleY(.33)}}.mit-checkbox-item{display:flex;flex-direction:row;justify-content:space-between;flex:1;padding-right:10rpx}.mit-checkbox-item-left{display:flex;justify-content:flex-start;flex-direction:row;align-items:center}.mit-checkbox-item-left-extra{font-size:30rpx;margin-right:30rpx}.mit-checkbox-item-left-extra .checked{color:#ff6b22}.mit-checkbox-item-left-extra .disabled,.mit-checkbox-item-left-extra .uncheck{color:#bbb}.mit-checkbox-item-left-title{line-height:1.5;width:auto}