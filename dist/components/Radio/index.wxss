.mit-radio-cell{display:flex;padding:30rpx;position:relative;border-bottom:1rpx solid #dddeee}@media (min-resolution:2dppx){.mit-radio-cell{border-bottom:none}.mit-radio-cell::after{content:'';position:absolute;background-color:#dddeee;display:block;z-index:2000;top:auto;right:auto;bottom:0;left:0;width:100%;height:1rpx;transform-origin:50% 100%;transform:scaleY(.5)}}.mit-radio-cell:active{background-color:#f3f3f3}.mit-radio-cell-last{border-bottom:0}.mit-radio-cell-last:after{display:none!important}@media (min-resolution:2dppx) and (min-resolution:3dppx){.mit-radio-cell::after{transform:scaleY(.33)}}.mit-radio-item{display:flex;flex-direction:row;justify-content:space-between;flex:1;padding-right:10rpx}.mit-radio-item-title{line-height:1.5;width:auto}.mit-radio-item-extra{font-size:30rpx}.mit-radio-item .default{color:#ff6b22;font-size:30rpx}.mit-radio-item .disabled{color:#bbb;font-size:30rpx}