.mit-cell-group-title{border-bottom:1rpx solid #dddeee;margin:0 30rpx;padding:30rpx 0;font-size:36rpx;color:#666}@media (min-resolution:2dppx){.mit-cell-group-title{border-bottom:none}.mit-cell-group-title::after{content:'';position:absolute;background-color:#dddeee;display:block;z-index:2000;top:auto;right:auto;bottom:0;left:0;width:100%;height:1rpx;transform-origin:50% 100%;transform:scaleY(.5)}}@media (min-resolution:2dppx) and (min-resolution:3dppx){.mit-cell-group-title::after{transform:scaleY(.33)}}